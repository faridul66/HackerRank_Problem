public static string hackerrankInString(string s)
    {
        string target="hackerrank";
        int count=0;
        for(int i=0;i<s.Length;i++){
            if(s[i]==target[count])
            {
                count++;
            }           
        }
        if(count==target.Length)
        {
            return "YES";
        }
        else{
            return "NO";
        }

    }
